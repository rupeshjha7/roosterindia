egex)!.map((x) => x.slice(1, -1));\r\n\r\n   headings.forEach((heading, i) => {\r\n      let paragraph = paragraphs[i];\r\n      res.push({ heading, text: paragraph });\r\n   });\r\n   return res;\r\n};\r\n\r\nexport const stringToParagraphs = (str: string) => {\r\n   return str.split(\"\\n\").map((x) => <div>{x}</div>);\r\n};\r\n\r\nexport const splitLines = (str: string) => {\r\n   return str.split(\"\\n\");\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\ImpactStoryCorrosionWedge.tsx",["569"],"import CaseStudyCenterCards from \"../CaseStudyCenterCards\";\r\nimport { CaseStudyPageProps } from \"../CaseStudyPage\";\r\nimport HeroBanner from \"./Impact_Story_Corrosion_Wedge/assets/Hero Banner.png\";\r\nimport { stringToHeaderCardProps, stringToParagraphs } from \"./utils\";\r\nimport ourApproachImage1 from \"./Impact_Story_Corrosion_Wedge/assets/01.svg\";\r\nimport ourApproachImage2 from \"./Impact_Story_Corrosion_Wedge/assets/02.svg\";\r\nimport ourApproachImage3 from \"./Impact_Story_Corrosion_Wedge/assets/03.svg\";\r\n\r\nimport { findAllByDisplayValue } from \"@testing-library/react\";\r\nexport const ImpactStoryCorrosionWedge: CaseStudyPageProps = {\r\n   path: \"/impact-story-corrosion-wedge\",\r\n   headerProps: {\r\n      parentIndustryPagePath: \"/manufacturing\",\r\n      mainHeading:\r\n         \"Ganit helps a global maufacturer restore high quality production line\",\r\n      image: HeroBanner,\r\n      headerCardParagraphs: stringToHeaderCardProps(\r\n         '\"99.99%\" Quality Assurance \"3 out of 1M\" lots received customer complaints'\r\n      ),\r\n   },\r\n\r\n   centerContentProps: {\r\n      subPages: [\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               Identifying the root-cause for quality defects is a big concern\r\n               for manufacturers\r\n            </div>\r\n            <div className=\"separated-paragraphs opensans-semibold\">\r\n               <div>\r\n                  The semiconductor manufacturing process involves a set of\r\n                  complex, high speed and micro-meter ranged processes along the\r\n                  entire assembly line. It hence becomes increasingly\r\n                  challenging for the manufacturers to identify and improve the\r\n                  processes that lead to production defects.\r\n               </div>\r\n               <div>\r\n                  Even after a through quality testing process, the manufacturer\r\n                  recevied multiple quality compaints from its customers over\r\n                  the last few months.\r\n               </div>\r\n               <div>\r\n                  The manufacturer sought Ganit's help to restore the production\r\n                  line, identify the bottlenecks and eliminate defective outputs\r\n               </div>\r\n            </div>\r\n         </div>,\r\n\r\n         <>\r\n            <div>\r\n               <div className=\"highlighted-paragraph\">\r\n                  Ganit used a combination of multi-variate statstical testing\r\n                  and rare-event modelling technique to determine the operating\r\n                  ranges of all machine parameters:\r\n               </div>\r\n               <div className=\"separated-paragraphs\">\r\n                  {stringToParagraphs(`Ganit worked with all the stakeholders - Product Heads, Manufacturing Director as well as the production engineers to identify the processes associated with all machines in the product line.\r\nUsing a hypothesis-based approach and statstical testing, we idetnified the parameters critical and having a high influence on the produciton quality.\r\nUsign advanced multi-variate statistical testing alogrithms, we determined the influence of combination of different process parameters on the final quality.`)}\r\n               </div>\r\n               <div className=\"highlighted-paragraph\">\r\n                  With the help of this we helped business achieve the follwing\r\n                  outcomes:\r\n               </div>\r\n               <CaseStudyCenterCards\r\n                  cards={[\r\n                     {\r\n                        image: ourApproachImage1,\r\n                        text:\r\n                           \"Dynamic ranges for SPC to ensure smooth production\",\r\n                     },\r\n                     {\r\n                        text:\r\n                           \"Identify and control the critical parameters to avoid defects\",\r\n                        image: ourApproachImage2,\r\n                     },\r\n                     {\r\n                        text: \"Ablility to correctly identify bad lots\",\r\n                        image: ourApproachImage3,\r\n                     },\r\n                  ]}\r\n               />\r\n            </div>\r\n         </>,\r\n         <>\r\n            <div>\r\n               The solution was piloted in 25 brick & mortor stores to observe\r\n               efficacy and effiency improvements. Solution will now be scaled\r\n               in a phased manner across categories and regions, ensuring that\r\n               on-ground teams are ready to adapt to the next-gen auto-indent\r\n               system.\r\n            </div>\r\n         </>,\r\n      ],\r\n   },\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\ImpactStoryLineBalancing.tsx",[],"C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\RouteOptimization.tsx",["570","571"],"import CaseStudyCenterCards from \"../CaseStudyCenterCards\";\r\nimport { CaseStudyPageProps } from \"../CaseStudyPage\";\r\nimport HeroBanner from \"./route_optimization/assests/Hero Banner.png\";\r\nimport { stringToHeaderCardProps, stringToParagraphs } from \"./utils\";\r\nimport ourApproachImage1 from \"./route_optimization/assests/01.svg\";\r\nimport ourApproachImage2 from \"./route_optimization/assests/02.svg\";\r\nimport ourApproachImage3 from \"./route_optimization/assests/03.svg\";\r\n\r\nimport ImpactImage from \"./route_optimization/assests/impact.png\";\r\nimport { findAllByDisplayValue } from \"@testing-library/react\";\r\nexport const RouteOptimization: CaseStudyPageProps = {\r\n   path: \"/route-optimization\",\r\n   id: \"route-optimization-page\",\r\n   headerProps: {\r\n      parentIndustryPagePath: \"/logistics\",\r\n      mainHeading:\r\n         \"A major logistics Company achieves 100 % of its SLA using Ganit's route optimizer and resource management solution.\",\r\n      image: HeroBanner,\r\n      headerCardParagraphs: stringToHeaderCardProps(\r\n         '\"15%\" Ganit\\'s route optimization solution leads to Zero missed deliveries and 15 % savings in costs'\r\n      ),\r\n   },\r\n\r\n   centerContentProps: {\r\n      subPages: [\r\n         <div>\r\n            <div className=\"separated-paragraphs opensans-semibold\">\r\n               <div>\r\n                  Large Players in the logistics space have SLAs attached in\r\n                  contracts with their customers, These SLAs usually come with a\r\n                  penalty clause attached which are invoked if the SLA is not\r\n                  met.\r\n               </div>\r\n               <div>\r\n                  The Client is a leader in the logistics market who had\r\n                  partnered with one of the world's largest online\r\n                  retilers.Prior to our engagement the client was penalized\r\n                  regularly for missed SLAs due to lack of resource management\r\n                  and route optimization\r\n               </div>\r\n               <div>\r\n                  We Identifed the need for data driven apparach in daily route\r\n                  planning, number of vehicles required and optimal assignment\r\n                  of shipments\r\n               </div>\r\n            </div>\r\n         </div>,\r\n\r\n         <>\r\n            <div>\r\n               <div className=\"highlighted-paragraph\">\r\n                  Ganit assessed the problem and designed a Route Optimizer with\r\n                  three key constraints which would dynamically provide clients\r\n                  with a daily route map and an effective plan.\r\n               </div>\r\n\r\n               <CaseStudyCenterCards\r\n                  cards={[\r\n                     {\r\n                        image: ourApproachImage1,\r\n                        text: \"Route Optimisation\",\r\n                     },\r\n                     {\r\n                        text: \"Vehicle Optimisation\",\r\n                        image: ourApproachImage2,\r\n                     },\r\n                     {\r\n                        text: \"Container Loading Optmisation\",\r\n                        image: ourApproachImage3,\r\n                     },\r\n                  ]}\r\n               />\r\n\r\n               <div className=\"separated-paragraphs paragraphs-with-headings\">\r\n                  <div>\r\n                     <h5>Route Optimisation</h5>\r\n                     <div>\r\n                        The solution approach was to arrive at an initial\r\n                        solution using greedy algorithm and then use\r\n                        metaheuristic algorithms to find an optimised solution\r\n                        within a predefined runtime\r\n                     </div>\r\n                  </div>\r\n                  <div>\r\n                     <h5>Vehicle Optimisation</h5>{\" \"}\r\n                     <div>\r\n                        Based on the dimensions of the shipments, a linear\r\n                        programming algoritmn was used to optimise the number of\r\n                        vehicles required.\r\n                     </div>\r\n                  </div>\r\n                  <div>\r\n                     <h5>Container Loading Optmisation</h5>\r\n                     <div>\r\n                        The container loading solution approach was a hybrid\r\n                        approach similar to route optimisation by using a\r\n                        heuristic and a metaheuristic to optimised solution\r\n                     </div>\r\n                  </div>\r\n               </div>\r\n            </div>\r\n         </>,\r\n         <>\r\n            <div className=\"separated-paragraphs paragraphs-with-headings\">\r\n               <div>\r\n                  <h5>Phase 1</h5>\r\n                  <div>\r\n                     The route optimizer was deployed on to the cloud and tested\r\n                     with a few selected territories.The model was then further\r\n                     fine tuned and calibrated.\r\n                  </div>\r\n               </div>\r\n               <div>\r\n                  <h5>Phase 2</h5>\r\n                  <div>\r\n                     The Final cloud based route optimizer was scaled and\r\n                     deployed across multiple locations which would act as a\r\n                     self service optimization tool for the end user\r\n                  </div>\r\n               </div>\r\n            </div>\r\n         </>,\r\n      ],\r\n   },\r\n   impactProps: {\r\n      image: ImpactImage,\r\n      paragraphs: [\r\n         \"There was a 15% savings in cost with zero missed deliveries. The delivery team no longer had to strech to meet the deadlines.\",\r\n         \"Number of vehicles used for delivery was brought down by 25% on average.\",\r\n      ],\r\n   },\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\WebsiteCPGDemandForeCasting.tsx",["572"],"import CaseStudyCenterCards from \"../CaseStudyCenterCards\";\r\nimport { CaseStudyPageProps } from \"../CaseStudyPage\";\r\nimport HeroBanner from \"./Website_CPG_DemandForecasting/assests/Hero Banner.png\";\r\nimport { stringToHeaderCardProps, stringToParagraphs } from \"./utils\";\r\n\r\nimport ImpactImage from \"./Website_CPG_DemandForecasting/assests/impact.png\";\r\nexport const WebsiteCPGDemandForeCasting: CaseStudyPageProps = {\r\n   path: \"/website-cpg-demand-fore-casting\",\r\n   headerProps: {\r\n      parentIndustryPagePath: \"/retail\",\r\n      mainHeading:\r\n         \"We helped a leading CPG company in South & East African markets develop a next-gen forecasting engine to better manage their supply chain\",\r\n      image: HeroBanner,\r\n      headerCardParagraphs: stringToHeaderCardProps(\r\n         '\"12%\" increase in forecast accuracy for key Categories contributing to 98% of revenue.'\r\n      ),\r\n   },\r\n\r\n   centerContentProps: {\r\n      subPages: [\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               With consumers expecting a high degree of variety in the products\r\n               hitting the shelves, product aisles are no longer lean. This has\r\n               made it increasingly difficult for CPG companies to manage their\r\n               supply chains.\r\n            </div>\r\n            <div className=\"separated-paragraphs opensans-semibold\">\r\n               {stringToParagraphs(`And often, teams struggle in developing a robust chain that delivers what the consumer seeks. However, digital adopters have made large strides by adopting advanced forecasting solutions to better manage their distribution networks.\r\nOur client operates in South and East African markets, supported by a network of production centers, logistics teams, regional sales teams, and localized sales representatives spread across strategic locations in these regions.\r\nThere was significant opportunity in driving more value out of the existing network by being able to anticipate future demands. This would help integrate sales and production teams, ensuring that key products would hit the shelves as expected.`)}\r\n            </div>\r\n         </div>,\r\n         <>\r\n            <div>\r\n               <div className=\"highlighted-paragraph\">\r\n                  We set out to solve three fundamental problems:\r\n                  <div>What to produce?</div>\r\n                  <div>How much to produce</div>\r\n                  <div>\r\n                     How to align all internal teams, from production teams to\r\n                     the on-ground salesforce teams?\r\n                  </div>\r\n               </div>\r\n\r\n               <div style={{ marginBottom: \"3.25em\" }}>\r\n                  We worked closely with business team & also on-ground store\r\n                  managers to develop an exhaustive list of 82 hypotheses.\r\n                  Testing these hypotheses was crucial in validating how data\r\n                  reflected some deeply held assumptions for the client, as well\r\n                  as for the larger industry. For example: Rainfall and Seasonal\r\n                  Availabilty\r\n               </div>\r\n               <div className=\"highlighted-paragraph\">\r\n                  We built an integrated 3-layer system to enable this solution\r\n                  at scale\r\n               </div>\r\n\r\n               <div className=\"separated-paragraphs paragraphs-with-headings\">\r\n                  <div>\r\n                     The first two problems “What to produce” and “How much to\r\n                     produce?” were solved by forecasting the revenue and units\r\n                     for SKUs across Categories for the next quarter. The third\r\n                     problem focused on aligning sales and production teams by\r\n                     letting them apply overrides within acceptable limits.\r\n                  </div>\r\n                  <div>\r\n                     <h5>Forecasting Engine: </h5>{\" \"}\r\n                     <div>\r\n                        Statistical Forecasting models were built to estimate\r\n                        the demand in the upcoming months\r\n                     </div>\r\n                  </div>\r\n                  <div>\r\n                     <h5>\r\n                        Tool to let Sales and Production teams to override\r\n                        forecast:\r\n                     </h5>\r\n                     <div>\r\n                        Our tool had embedded levers to account for current\r\n                        market trends and inputs from the salesforce teams. This\r\n                        helped the production and sales teams to effectively\r\n                        reconcile ground intelligence with the statistical\r\n                        forecasts generated\r\n                     </div>\r\n                  </div>\r\n                  <div>\r\n                     <h5>Workflow Orchestration</h5>\r\n                     <div>\r\n                        We orchestrated the processes to take in inputs from the\r\n                        salesforce team, and automated the workflow to deliver\r\n                        consolidated reports and forecasts to the entire team\r\n                     </div>\r\n                  </div>\r\n               </div>\r\n            </div>\r\n         </>,\r\n         <>\r\n            <div>\r\n               The solution was first piloted across a country to validate the\r\n               accuracy of our forecasts and to scout for process\r\n               improvements.The solution was then scaled up to various countries\r\n               in a phased manner across East and South African markets.\r\n            </div>\r\n         </>,\r\n      ],\r\n   },\r\n   impactProps: {\r\n      image: ImpactImage,\r\n      paragraphs: [\r\n         \"The automated engine shortened the quarterly planning cycle, and also made it easier for internal teams to set out accurate production targets. \",\r\n      ],\r\n   },\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\WebsiteCPGRecommendationEngine.tsx",[],"C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\WebsiteRHBrandMonitor.tsx",["573"],"import CaseStudyCenterCards from \"../CaseStudyCenterCards\";\r\nimport { CaseStudyPageProps } from \"../CaseStudyPage\";\r\nimport HeroBanner from \"./Website_RH_BrandMonitor/assets/Hero Banner.png\";\r\nimport { stringToHeaderCardProps, stringToParagraphs } from \"./utils\";\r\n\r\nimport ImpactImage from \"./Website_RH_BrandMonitor/assets/impact.png\";\r\nexport const WebsiteRHBrandMonitor: CaseStudyPageProps = {\r\n   path: \"/website-rbh-brand-monitor\",\r\n   headerProps: {\r\n      mainHeading:\r\n         \"Ganit designs a Brand Monitor to help a leading QSR chain track customer sentiment and prescribe measures to improve customer enagement\",\r\n      image: HeroBanner,\r\n      headerCardParagraphs: stringToHeaderCardProps(\r\n         '\"1 week to 1day\" Reduced Man Hours from 1 week to 1 day. Tailored Bra. Monitor with Real-tIme data capture. Prescribed Customer Win-Back Strategies'\r\n      ),\r\n   },\r\n\r\n   centerContentProps: {\r\n      subPages: [\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               Internet reviews can make or break a brand. Keeping track of\r\n               reviews, often spread across multiple websites and platforms is\r\n               time-consuming and daunting.\r\n            </div>\r\n            <div className=\"separated-paragraphs opensans-semibold\">\r\n               {stringToParagraphs(`With an increasing number of crowd-sourced review sites, restaurant businesses can get real time customer feedback, and rely on these sites to develop momentum for growth. Operating across 35 multi-format outlets in India, it was evident that there was a lot of uncapitalized data about the client's brand. However, they struggled with tackling the volume of data feeding in across food aggregators, review sites, blogs, news sites etc \r\nThe clients wanted help in designing a platform that would help understand brand perception and track customer sentiment. `)}\r\n            </div>\r\n         </div>,\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               We worked with the client to develop a real-time Brand Monitor to\r\n               track brand perception, and derive actionable insights from\r\n               customer reviews and feedback.\r\n            </div>\r\n            <div className=\"separated-paragraphs\">\r\n               <div>\r\n                  We they focused on two tactical measures to ensure sustainable\r\n                  customer engagement. The first measure focused on winning back\r\n                  customers who had a sub-par experience, while the second\r\n                  focused on retaining customers who had a positive experience.\r\n               </div>\r\n               <div>\r\n                  The Brand Monitor also tracks all user feedback on a periodic\r\n                  basis, giving the Brand Marketers an instant feedback on how\r\n                  customers are responding. We also embedded an outlet-level\r\n                  dashboard to track customer sentiment across 30+ stores.{\" \"}\r\n               </div>\r\n            </div>\r\n         </div>,\r\n         <>\r\n            <div>\r\n               In the design phase, we worked closely with the Brand Managers to\r\n               capture their pain points. This was pivotal in designing the\r\n               Brand Monitor with embedded decision boards to take decisions\r\n               based on real time data feeding in. To enable a pan-India roll\r\n               out, we also worked with Store managers to layer in\r\n               outlet-specific views for various markets.\r\n            </div>\r\n         </>,\r\n      ],\r\n   },\r\n   impactProps: {\r\n      image: ImpactImage,\r\n      paragraphs: [\r\n         \"Live Cockpit to monitor Voice of Customer from over 50 platforms and sources. Automated platform brought down the feedback cycle to just one day. \",\r\n      ],\r\n   },\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\WebsiteRHCombo.tsx",["574"],"import CaseStudyCenterCards from \"../CaseStudyCenterCards\";\r\nimport { CaseStudyPageProps } from \"../CaseStudyPage\";\r\nimport HeroBanner from \"./Website_RH_Combo/assets/Hero Banner.png\";\r\nimport { stringToHeaderCardProps, stringToParagraphs } from \"./utils\";\r\n\r\nimport ImpactImage from \"./Website_RH_Combo/assets/Impant.png\";\r\nexport const WebsiteRHCombo: CaseStudyPageProps = {\r\n   path: \"/website-rh-combo\",\r\n   headerProps: {\r\n      mainHeading:\r\n         \"One of the biggest food chains in India partners with Ganit to deploy a data-driven system to introduce value-based offerings \",\r\n      image: HeroBanner,\r\n      headerCardParagraphs: stringToHeaderCardProps(\r\n         '\"22%\" Increase in Revenue \"\" Piloted Recommendations across 35 multi-format stores'\r\n      ),\r\n   },\r\n\r\n   centerContentProps: {\r\n      subPages: [\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               Combos are a great way to boost upsell and cross-sell\r\n               opportunities for restaurants. While combos might seem like an\r\n               easy route to cash in on increasing consumer wallet sizes, the\r\n               lack of balanced and systematically identified combos could drive\r\n               customers across the aisle.\r\n            </div>\r\n            <div className=\"separated-paragraphs opensans-semibold\">\r\n               {stringToParagraphs(`Our client, one of the biggest QSR chains in India wanted to introduce combos to its current offerings. However, with more than 35+ outlets, identifying the right set of combos across various channels such as dine-in, takeaway, and online-delivery was a major challenge. \r\nThe client wanted to utilise Ganit's data science capabilities to strategically craft combos that could increase traction for the combo line-up while also ensuring profitability. `)}\r\n            </div>\r\n         </div>,\r\n         <>\r\n            <div>\r\n               <div className=\"highlighted-paragraph\">\r\n                  We worked closely with the store managers to map out customer\r\n                  preferences and buying patterns.\r\n               </div>\r\n\r\n               <div className=\"separated-paragraphs\">\r\n                  {stringToParagraphs(`Advanced Machine learning algorithms helped identify combos that could drive value for the customer, and at the same time ensure profitability for the brand. Customised metrics were designed to zero in on these combos line-ups. \r\nDesigned the right offering also accounted for the channel of operation, number of items to be bundled and number of pax. per ticket. Our recommendations were incorporated across all 30+ stores in India.`)}\r\n               </div>\r\n            </div>\r\n         </>,\r\n         <>\r\n            <div>\r\n               Designed multiple value-based combo variants and promotional\r\n               offers to test customer adoption across various operating\r\n               channels. Best received offerings were scaled pan-India as\r\n               permanent menu combos.\r\n            </div>\r\n         </>,\r\n      ],\r\n   },\r\n   impactProps: {\r\n      image: ImpactImage,\r\n      paragraphs: [\r\n         \"Our ML algorithms delivered a 22% increase in revenue through direct cross-sell gains, thus increasing average ticket size.  Test stores also saw an increase in customer footfall by 5%. \",\r\n      ],\r\n   },\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\CaseStudyPage\\AllCaseStudies\\MediaImpact.tsx",["575"],"import CaseStudyCenterCards from \"../CaseStudyCenterCards\";\r\nimport { CaseStudyPageProps } from \"../CaseStudyPage\";\r\nimport HeroBanner from \"./media impact/assets/Hero Banner.png\";\r\nimport { stringToHeaderCardProps } from \"./utils\";\r\n\r\nimport enableConsumptionImage1 from \"./media impact/assets/01.svg\";\r\nimport enableConsumptionImage2 from \"./media impact/assets/02.svg\";\r\nimport enableConsumptionImage3 from \"./media impact/assets/03.svg\";\r\nimport enableConsumptionImage4 from \"./media impact/assets/04.svg\";\r\n\r\nimport ImpactImage from \"./media impact/assets/impact.png\";\r\nimport CaseStudyCenterCard from \"../CaseStudyCenterCard\";\r\nexport const MediaImpact: CaseStudyPageProps = {\r\n   path: \"/fv-project\",\r\n   headerProps: {\r\n      mainHeading:\r\n         \"An Indian media conglomerate which is a wholly owned subsidiary of one of the largest media behemoths in the world had taken our support to understand the preferences and behaviour of viewers when watching TV for selling the inventory to the right customer and at the right price\",\r\n      image: HeroBanner,\r\n      headerCardParagraphs: stringToHeaderCardProps(\r\n         '\"500M\" Incremental revenue of 500 million rupees as well as reducing the time for action by over a month.'\r\n      ),\r\n   },\r\n\r\n   centerContentProps: {\r\n      subPages: [\r\n         <div className=\"separated-paragraphs \">\r\n            <div>\r\n               Media Broadcasting Companies are extremely reliant on industry\r\n               experts and on BARC data for insights which has been the primary\r\n               source of data for mainstream media despite some of its\r\n               limitations.\r\n            </div>\r\n            <div>\r\n               The client has setup up an alternative source of data in\r\n               collaboration with a US based startup manufacturing IOT based\r\n               viewership device enabling the flow of more granular data. The\r\n               client wants to make best use of the alternate source of\r\n               information to fill the knowledge gaps which would help in\r\n               selling the inventory at better pricing.\r\n            </div>\r\n            <div>\r\n               The IOT viewership data may subsequently replace BARC data as the\r\n               primary source of viewership information and then BARC would be\r\n               made supplementary. The predictive models and analysis built on\r\n               top of this data is one of the primary factors that provides the\r\n               decision-making power to the team on how to sell the inventory\r\n               and at what price. As the technology is in its infancy, Advanced\r\n               statistical/ML techniques are required to de-noise the data and\r\n               to identify and conclusively prove the outcomes. The activity is\r\n               time critical as unsold or underpriced inventory will result in a\r\n               huge financial loss for the client\r\n            </div>\r\n         </div>,\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               Ganit assessed the problem, breaking it down into key\r\n               compartments\r\n            </div>\r\n            <div className=\"separated-paragraphs paragraphs-with-headings\">\r\n               <div>\r\n                  <h5>Creating data dictionary ? Understanding the data?</h5>\r\n                  <div>\r\n                     Analysed almost 1 billion records of which 60% were\r\n                     Programs and 40% were ads.The data was from 125 chanells\r\n                     across 33 genres.\r\n                  </div>\r\n               </div>\r\n               <div>\r\n                  <div>In total we had</div>\r\n                  <div>\r\n                     ~275,000 hours of content recorded whenever the TV is ON\r\n                  </div>\r\n                  <div>\r\n                     ~0.2 million unique Program Airings across all channels and\r\n                     day parts\r\n                  </div>\r\n                  <div>\r\n                     ~3 million unique AD Airings across all channels and day\r\n                     parts\r\n                  </div>\r\n               </div>\r\n               <div>\r\n                  <h5>\r\n                     Integration and Synchronization of seemingly disparate data\r\n                     sources\r\n                  </h5>\r\n                  <div>\r\n                     Analysed almost 1 billion records of which 60% were\r\n                     Programs and 40% were ads.The data was from 125 chanells\r\n                     across 33 genres.\r\n                  </div>\r\n               </div>\r\n               <div>\r\n                  <div>\r\n                     Data is captured every second and the content is recognized\r\n                     using Audio Content Recognition.\r\n                  </div>\r\n                  <div>\r\n                     A camera captures video data and every viewer’s presence\r\n                     and eyes on screen is tracked using advanced AI and ML\r\n                     algorithms\r\n                  </div>\r\n                  <div>\r\n                     The data from every household gets transferred to a data\r\n                     lake\r\n                  </div>\r\n                  <div>\r\n                     The data is ETL’ed from Data Lake and then used for further\r\n                     analysis\r\n                  </div>\r\n               </div>\r\n            </div>\r\n         </div>,\r\n         <div>\r\n            <div className=\"highlighted-paragraph\">\r\n               Our hypothesis based approach yielded some powerful insights on\r\n            </div>\r\n            <CaseStudyCenterCards\r\n               cards={[\r\n                  {\r\n                     image: enableConsumptionImage1,\r\n                     text: \"Decay Effect of an AD pod by Duration\",\r\n                  },\r\n                  {\r\n                     image: enableConsumptionImage2,\r\n                     text: \"Effect of having celebrity on Ads\",\r\n                  },\r\n                  {\r\n                     image: enableConsumptionImage3,\r\n                     text: \"Viewer AD Fatigue\",\r\n                  },\r\n                  {\r\n                     image: enableConsumptionImage4,\r\n                     text: \"Effect of genres on ADS\",\r\n                  },\r\n               ]}\r\n            ></CaseStudyCenterCards>\r\n         </div>,\r\n      ],\r\n   },\r\n   impactProps: {\r\n      image: ImpactImage,\r\n      paragraphs: [\r\n         \"Incremental revenue of 500 million rupees as well as reducing the time for action by over a month from one of Indias most watched sports event\",\r\n         \"A significantly large number of hypothesis were tested over and above what was initially planned as we completed the tasks in 15 days which initially was scheduled to be completed in 90 days.\",\r\n      ],\r\n   },\r\n};\r\n","C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Routes.tsx",[],"C:\\Users\\HB Laptop point\\Documents\\codes\\ludo\\src\\Pages\\Menu\\MenuMobile.tsx",["576","577"],"import * as React from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { useStateContext } from \"../../Contexts/useGameContext\";\r\nimport CloseBtnSVG from \"./CloseBtnSVG\";\r\nimport {\r\n   caseStudyList1,\r\n   menuItemList2,\r\n   caseStudyList2,\r\n   menuItemList1,\r\n} from \"./MenuData\";\r\nimport MenuLinkItem from \"./MenuLinkItem\";\r\nimport MenuSubLinkItem from \"./MenuSubLinkItem\";\r\n\r\nexport interface MenuMobileProps {}\r\nconst menuItemList = [...menuItemList1, ...menuItemList2];\r\nconst menuCaseStudyList = [...caseStudyList1, ...caseStudyList2];\r\nconst MenuMobile: React.FC = () => {\r\n   const [isCaseStudyListShowing, setIsCaseStudyListShowing] = React.useState(\r\n      false\r\n   );\r\n   const [state, dispatch] = useStateContext();\r\n   return (\r\n      <div\r\n         className=\"d-flex flex-column page menu-mobile pb-5\"\r\n         id=\"menu-mobile\"\r\n      >\r\n         <div className=\"d-flex pb-5\">\r\n            <div\r\n               className=\"anv-bold mb-mb-1\"\r\n               style={{ fontSize: \"1.875em\", marginBottom: \"3em\" }}\r\n            >\r\n               Look out for\r\n            </div>\r\n            <div\r\n               onClick={() => dispatch({ \"hide-sidebar\": \"\" })}\r\n               className=\"cursor-pointer bg-blue d-flex justify-content-center align-items-center\"\r\n               style={{\r\n                  marginLeft: \"auto\",\r\n                  justifySelf: \"flex-end\",\r\n                  height: \"3.5em\",\r\n                  width: \"3.5em\",\r\n               }}\r\n            >\r\n               <CloseBtnSVG></CloseBtnSVG>\r\n            </div>\r\n         </div>\r\n         <div className=\"d-flex flex-column\">\r\n            <div className=\"d-flex flex-column\" style={{ marginBottom: \"4em\" }}>\r\n               {menuItemList.slice(0, 2).map((item) => (\r\n                  <MenuLinkItem {...item} />\r\n               ))}\r\n               <MenuLinkItem\r\n                  subtext=\"\"\r\n                  text=\"Industry Specific Casestudies\"\r\n                  onClick={() =>\r\n                     setIsCaseStudyListShowing(!isCaseStudyListShowing)\r\n                  }\r\n               ></MenuLinkItem>\r\n               <div\r\n                  className=\"flex-column my-4\"\r\n                  style={{ display: isCaseStudyListShowing ? \"flex\" : \"none\" }}\r\n               >\r\n                  {menuCaseStudyList.map((item) => (\r\n                     <MenuSubLinkItem {...item}></MenuSubLinkItem>\r\n                  ))}\r\n               </div>\r\n               {menuItemList.slice(2).map((item) => (\r\n                  <MenuLinkItem {...item} />\r\n      